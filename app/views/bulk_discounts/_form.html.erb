<%= form_with model: model, method: method, url: url, local: true do |f| %>
  <%= f.label "Percent off:" %>
  <%= f.number_field :percentage, max: 99, value: perc_value %><br/><br/>
  <%= f.label "Quantity threshold:" %>
  <%= f.number_field :quantity, value: quan_value %><br/><br/>
  <%= f.submit 'Submit'%>
<% end %>